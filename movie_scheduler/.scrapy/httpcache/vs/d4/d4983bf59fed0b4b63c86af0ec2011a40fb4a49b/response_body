    <script type="text/javascript">
        document.getElementById('browserBar').style.visibility='visible';
        document.getElementById('browser').style.visibility='visible';
        document.getElementById('close').onclick=closePopup;
        function closePopup(){
        document.getElementById('browser').style.visibility='hidden';
        }
    </script><![endif]-->

  </body>
</html>